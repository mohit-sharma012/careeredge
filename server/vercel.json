{
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000",
    "builds": [
      {
        "src": "/src/main.py",
        "use": "@vercel/python",
        "config": {
            "buildCommand": "pip install -r requirements/requirements.txt"
          }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/main.py"
      }
    ]
  }